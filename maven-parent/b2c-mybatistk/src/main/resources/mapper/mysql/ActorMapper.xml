<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.javamaster.b2c.cloud.test.mybatistk.mapper.ActorMapper">
    <select id="findAll" resultType="org.javamaster.b2c.mybatistk.model.Actor">
        select actor_id actorId, first_name firstName, last_name lastName, last_update lastUpdate
        from actor
    </select>
</mapper>